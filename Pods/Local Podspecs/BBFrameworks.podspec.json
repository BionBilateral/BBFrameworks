{
  "name": "BBFrameworks",
  "version": "0.0.1",
  "authors": {
    "William Towe": "will@bionbilateral.com",
    "Jason Anderson": "jason@bionbilateral.com"
  },
  "license": {
    "type": "BSD",
    "file": "license.txt"
  },
  "homepage": "https://github.com/BionBilateral/BBFrameworks",
  "source": {
    "git": "git@github.com:BionBilateral/BBFrameworks.git",
    "tag": "0.0.1"
  },
  "summary": "Repository for common iOS/OSX categories, classes, and functions.",
  "platforms": {
    "ios": "8.3",
    "osx": "10.10"
  },
  "requires_arc": true,
  "dependencies": {
    "BlocksKit/Core": [
      "~> 2.2.0"
    ]
  },
  "subspecs": [
    {
      "name": "BBFoundation",
      "source_files": "BBFrameworks/BBFoundation",
      "frameworks": "Foundation"
    },
    {
      "name": "BBCoreData",
      "dependencies": {
        "BBFrameworks/BBFoundation": [

        ]
      },
      "source_files": "BBFrameworks/BBCoreData",
      "ios": {
        "frameworks": [
          "CoreData",
          "UIKit"
        ]
      },
      "osx": {
        "frameworks": [
          "CoreData",
          "AppKit"
        ]
      }
    },
    {
      "name": "BBKit",
      "dependencies": {
        "Archimedes": [
          "~> 1.1.0"
        ],
        "BBFrameworks/BBFoundation": [

        ]
      },
      "source_files": "BBFrameworks/BBKit",
      "ios": {
        "exclude_files": [
          "NSColor+BBKitExtensions.{h,m}",
          "NSImage+BBKitExtensions.{h,m}"
        ],
        "frameworks": [
          "UIKit",
          "Accelerate",
          "AVFoundation"
        ]
      },
      "osx": {
        "exclude_files": [
          "UIColor+BBKitExtensions.{h,m}",
          "UIImage+BBKitExtensions.{h,m}",
          "UIView+BBKitExtensions.{h,m}",
          "UIViewController+BBKitExtensions.{h,m}",
          "UIFont+BBKitExtensions.{h,m}",
          "UIBarButtonItem+BBKitExtensions.{h,m}"
        ],
        "frameworks": [
          "AppKit",
          "Accelerate",
          "AVFoundation"
        ]
      }
    },
    {
      "name": "BBReactiveKit",
      "dependencies": {
        "ReactiveCocoa": [
          "~> 2.5.0"
        ],
        "ReactiveViewModel": [
          "~> 0.3.0"
        ],
        "BBFrameworks/BBKit": [

        ]
      },
      "ios": {
        "dependencies": {
          "TUSafariActivity": [
            "~> 1.0.0"
          ],
          "ARChromeActivity": [
            "~> 1.0.0"
          ]
        }
      },
      "source_files": "BBFrameworks/BBReactiveKit",
      "osx": {
        "exclude_files": [
          "BBProgressNavigationBar.{h,m}",
          "BBWebKitViewController.{h,m}",
          "BBWebKitViewControllerDelegate.h",
          "BBWebKitTitleView.{h,m}"
        ]
      }
    },
    {
      "name": "BBThumbnail",
      "dependencies": {
        "ReactiveCocoa": [
          "~> 2.5.0"
        ],
        "BBFrameworks/BBKit": [

        ]
      },
      "source_files": "BBFrameworks/BBThumbnail",
      "ios": {
        "frameworks": "MobileCoreServices"
      },
      "osx": {
        "frameworks": "QuickLook"
      }
    },
    {
      "name": "BBReactiveThumbnail",
      "dependencies": {
        "BBFrameworks/BBThumbnail": [

        ]
      },
      "source_files": "BBFrameworks/BBReactiveThumbnail"
    }
  ]
}
